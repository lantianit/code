<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.mapper.CarouselImageMapper">

    <resultMap id="BaseResultMap" type="org.example.entity.CarouselImage">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="name" column="name" jdbcType="VARCHAR"/>
            <result property="imgurl" column="imgUrl" jdbcType="VARCHAR"/>
            <result property="imgorder" column="order" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,name,imgUrl,
        order
    </sql>
</mapper>
